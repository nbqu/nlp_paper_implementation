{
    "rnn_input_size": 512,
    "rnn_hidden_size": 512,
    "rnn_num_layers": 4,
    "rnn_bidirectional": true,

    "sampling_ratio": 0.5,

    "learning_rate": 0.7,
    "lr_decay_at": 5,

    "src_lang": "fr",
    "tgt_lang": "en",
    "src_tokenizer_path": "./seq2seq/fr_tokenizer",
    "tgt_tokenizer_path": "./seq2seq/en_tokenizer",

    "gradient_clip_val": 5,
    "max_epochs": 8
}
